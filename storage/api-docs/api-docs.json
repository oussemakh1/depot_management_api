    "components": {
        "schemas": {
            "DepotSchema": {
                "title": "Depot Model",
                "description": "Depot Model",
                "properties": {
                    "id": {
                        "description": "depot primary key",
                        "type": "uint64",
                        "example": "1"
                    },
                    "location": {
                        "description": "depot location (country, city, address)",
                        "type": "string",
                        "example": "tunis, sfax route tanyour km6"
                    },
                    "size": {
                        "description": "depot size",
                        "type": "string",
                        "example": "120x40mÂ²"
                    },
                    "capacity": {
                        "description": "depot capacity of storing",
                        "type": "integer",
                        "example": "500"
                    },
                    "type": {
                        "description": "depot products stored type",
                        "type": "string",
                        "example": "food"
                    },
                    "isRented": {
                        "description": "depot rented verifier",
                        "type": "boolean",
                        "example": "1"
                    },
                    "rent": {
                        "description": "depot renting cost",
                        "type": "float",
                        "example": "1520.47"
                    }
                },
                "type": "object"
            },
            "InvoiceSchema": {
                "title": "Invoice Model",
                "description": "Invoice Model",
                "properties": {
                    "product_id": {
                        "description": "product id",
                        "type": "integer",
                        "example": "1"
                    },
                    "provider_id": {
                        "description": "provider id",
                        "type": "integer",
                        "example": "1"
                    },
                    "status": {
                        "description": "invoice status",
                        "type": "string",
                        "example": "pending"
                    },
                    "quantity": {
                        "description": "product quantity",
                        "type": "integer",
                        "example": "10"
                    },
                    "recivedat": {
                        "description": "product delivery date",
                        "type": "date",
                        "example": "2021/08/11 11:045:20"
                    },
                    "price": {
                        "description": "product price",
                        "type": "decimal",
                        "example": "455.2"
                    },
                    "discount": {
                        "description": "product discount",
                        "type": "decimal",
                        "example": "1500"
                    },
                    "total": {
                        "description": "invoice total",
                        "type": "decimal",
                        "example": "1500.57"
                    }
                },
                "type": "object"
            },
            "ProductSchema": {
                "title": "Product Model",
                "description": "Product Model",
                "properties": {
                    "name": {
                        "description": "product name",
                        "type": "string",
                        "example": "samasung A20"
                    },
                    "size": {
                        "description": "product size",
                        "type": "string",
                        "example": "20x40mm"
                    },
                    "weight": {
                        "description": "product weight",
                        "type": "decimal",
                        "example": "54g"
                    },
                    "cost": {
                        "description": "product cost",
                        "type": "decimal",
                        "example": "220"
                    },
                    "quantity": {
                        "description": "product quantity",
                        "type": "integer",
                        "example": "25"
                    },
                    "type": {
                        "description": "product type",
                        "type": "string",
                        "example": "smartphone"
                    },
                    "expiredat": {
                        "description": "product expiration date",
                        "type": "date",
                        "example": "2021/04/11"
                    },
                    "depot_id": {
                        "description": "product depot",
                        "type": "integer",
                        "example": "2"
                    }
                },
                "type": "object"
            },
            "ProviderSchema": {
                "title": "Provider Model",
                "description": "Provider Model",
                "properties": {
                    "name": {
                        "description": "provider name",
                        "type": "string",
                        "example": "samsung corp"
                    },
                    "email": {
                        "description": "provider email",
                        "type": "string",
                        "example": "contact@samsung.com"
                    },
                    "fax": {
                        "description": "provider fax",
                        "type": "string",
